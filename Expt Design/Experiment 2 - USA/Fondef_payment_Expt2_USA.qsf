         sliderCount9: 0\n        };\n\n        \/\/ Method to update the count\n        function updateCount(countVariableName) {\n            counts[countVariableName]++;\n\t\t\t\/\/ To log the count\n\t\t\tvar countsObject = sliderCounts.getCounts();\n\t\t\tconsole.log(countsObject.sliderCount1, countsObject.sliderCount2, countsObject.sliderCount3, countsObject.sliderCount4, countsObject.sliderCount5, countsObject.sliderCount6, countsObject.sliderCount7, countsObject.sliderCount8, countsObject.sliderCount9);\n        }\n\n        \/\/ Method to get the counts object\n        function getCounts() {\n            return counts;\n        }\n\n        \/\/ Return an object with the public methods\n        return {\n            updateCount: updateCount,\n            getCounts: getCounts\n        };\n    }\n\n    \/\/ Usage example\n    var sliderCounts = SliderCounts();\n    slider.oninput = function() {\n        var balance = parseFloat(\"${e:\/\/Field\/balance}\");\n        var previousPayment = payment; \/\/ Store the previous payment value\n        payment = parseFloat(this.value);\n\n        \/\/ Update the mpwtable with the time to pay off the debt\n        jQuery(\"#paymentDisplay\").text(\"$\" + payment.toLocaleString('en-US'));\n\n        var sliderMin = parseFloat(slider.min);\n        var sliderMax = parseFloat(slider.max);\n        var sliderLength = sliderMax - sliderMin;\n        var percentage1 = sliderMin + sliderLength * 0.1;\n        var percentage2 = sliderMin + sliderLength * 0.2;\n        var percentage3 = sliderMin + sliderLength * 0.3;\n        var percentage4 = sliderMin + sliderLength * 0.4;\n        var percentage5 = sliderMin + sliderLength * 0.5;\n        var percentage6 = sliderMin + sliderLength * 0.6;\n        var percentage7 = sliderMin + sliderLength * 0.7;\n        var percentage8 = sliderMin + sliderLength * 0.8;\n        var percentage9 = sliderMin + sliderLength * 0.9;\n\n\t\t\/\/ Check if the slider passes different percentage poin